      "description": "allow to DHCP server"
     }, 
     "3002": {
      "action": "accept", 
      "protocol": "icmp", 
      "description": "allow ICMP"
     }
    }
   }, 
   "LAN_LOCAL": {
    "default-action": "accept", 
    "description": "packets from intranet to gateway"
   }, 
   "WAN_IN": {
    "default-action": "drop", 
    "description": "packets from internet to intranet", 
    "rule": {
     "3001": {
      "action": "accept", 
      "state": {
       "established": "enable", 
       "related": "enable", 
       "invalid": "disable", 
       "new": "disable"
      }, 
      "description": "allow established/related sessions"
     }, 
     "3002": {
      "action": "drop", 
      "state": {
       "established": "disable", 
       "related": "disable", 
       "invalid": "enable", 
       "new": "disable"
      }, 
      "description": "drop invalid state"
     }
    }
   }, 
   "AUTHORIZED_GUESTS": {
    "default-action": "drop", 
    "description": "authorization check packets from guest network"
   }, 
   "GUEST_OUT": {
    "default-action": "accept", 
    "description": "packets forward to guest network"
   }, 
   "WAN_LOCAL": {
    "default-action": "drop", 
    "description": "packets from internet to gateway", 
    "rule": {
     "3001": {
      "action": "accept", 
      "state": {
       "established": "enable", 
       "related": "enable", 
       "invalid": "disable", 
       "new": "disable"
      }, 
      "description": "allow established/related sessions"
     }, 
     "3002": {
      "action": "drop", 
