{
 "service": {
  "lldp": {
   "interface": {
    "eth3": {
     "disable": "''"
    }, 
    "eth2": {
     "disable": "''"
    }
   }
  }, 
  "nat": {
   "rule": {
    "6006": {
     "protocol": "all", 
     "description": "MASQ remote_user_vpn_network to WAN", 
     "outbound-interface": "pppoe1", 
     "source": {
      "group": {
       "network-group": "remote_user_vpn_network"
      }
     }, 
     "type": "masquerade", 
     "log": "disable"
    }, 
    "6007": {
     "protocol": "all", 
     "description": "MASQ guest_network to WAN", 
     "outbound-interface": "pppoe1", 
     "source": {
      "group": {
       "network-group": "guest_network"
      }
     }, 
     "type": "masquerade", 
     "log": "disable"
    }, 
    "6004": {
     "protocol": "all", 
     "description": "MASQ pppoe1 out other WAN", 
     "outbound-interface": "eth2", 
     "source": {
      "group": {
       "address-group": "ADDRv4_pppoe1"
      }
     }, 
     "type": "masquerade", 
     "log": "disable"
    }, 
    "6005": {
     "protocol": "all", 
     "description": "MASQ corporate_network to WAN", 
     "outbound-interface": "pppoe1", 
     "source": {
      "group": {
       "network-group": "corporate_network"
      }
     }, 
     "type": "masquerade", 
     "log": "disable"
    }, 
    "6002": {
     "protocol": "all", 
     "description": "MASQ remote_user_vpn_network to WAN", 
     "outbound-interface": "eth2", 
     "source": {
      "group": {
       "network-group": "remote_user_vpn_network"
      }
     }, 
     "type": "masquerade", 
     "log": "disable"
    }, 
    "6003": {
     "protocol": "all", 
     "description": "MASQ guest_network to WAN", 
     "outbound-interface": "eth2", 
     "source": {
      "group": {
       "network-group": "guest_network"
      }
     }, 
     "type": "masquerade", 
     "log": "disable"
    }, 
    "6001": {
     "protocol": "all", 
     "description": "MASQ corporate_network to WAN", 
     "outbound-interface": "eth2", 
     "source": {
      "group": {
       "network-group": "corporate_network"
      }
     }, 
     "type": "masquerade", 
     "log": "disable"
    }, 
    "6008": {
     "protocol": "all", 
     "description": "MASQ eth2 out other WAN", 
     "outbound-interface": "pppoe1", 
     "source": {
      "group": {
       "address-group": "ADDRv4_eth2"
      }
     }, 
     "type": "masquerade", 
     "log": "disable"
    }
   }
  }, 
  "gui": {
   "https-port": "443"
  }, 
  "ssh": {
   "port": "22", 
   "protocol-version": "v2"
  }, 
  "dns": {
   "forwarding": {
    "options": [
     "all-servers", 
     "cname=unifi.localdomain,unifi", 
     "resolv-file=/etc/ppp/resolv.conf", 
     "resolv-file=/etc/resolv.conf.dhclient-new-eth2", 
     "server=1.1.1.1"
    ], 
    "cache-size": "10000", 
    "except-interface": [
     "eth2", 
     "pppoe1"
    ]
   }
  }, 
  "dhcp-server": {
   "disabled": "false", 
   "hostfile-update": "enable", 
   "global-parameters": [
    "class &quot;denied&quot; { match substring (hardware, 1, 6); deny booting; } subclass &quot;denied&quot; 80:2a:a8:cd:a9:53; subclass &quot;denied&quot; 80:2a:a8:cd:a9:54;"
   ], 
   "shared-network-name": {
    "net_LAN_eth0_192.168.1.0-24": {
     "authoritative": "enable", 
     "subnet": {
      "192.168.1.0/24": {
       "start": {
        "192.168.1.6": {
         "stop": "192.168.1.254"
        }
       }, 
       "domain-name": "localdomain", 
       "default-router": "192.168.1.1", 
       "dns-server": [
        "192.168.1.1"
       ], 
       "lease": "86400"
      }
     }, 
     "description": "vlan1"
    }
   }
  }
 }, 
 "load-balance": {
  "group": {
   "wan_failover": {
    "interface": {
     "eth2": {
      "weight": 50, 
      "route-test": {
       "initial-delay": "20", 
       "interval": "10"
      }
     }, 
     "pppoe1": {
      "weight": 50, 
      "route-test": {
       "initial-delay": "20", 
       "interval": "10"
      }
     }
    }, 
    "sticky": {
     "source-addr": "enable", 
     "dest-addr": "enable", 
     "dest-port": "enable"
    }
   }
  }
 }, 
